<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.farid.projectindividu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectIndividu"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".SearchInstrukturActivity"
            android:exported="false" />
        <activity
            android:name=".SearchPesertaActivity"
            android:exported="false" />
        <activity
            android:name=".ui.detailkelas.TambahDataDetailKelasActivity"
            android:exported="false" />
        <activity
            android:name=".ui.detailkelas.LihatDataDetailKelasActivity"
            android:exported="false" />
        <activity
            android:name=".ui.kelas.LihatDetailKelasActivity"
            android:exported="false" />
        <activity
            android:name=".ui.kelas.TambahKelasActivity"
            android:exported="false" />
        <activity
            android:name=".ui.instruktur.LihatDataInstrukturActivity"
            android:exported="false" />
        <activity
            android:name=".ui.instruktur.TambahInstrukturActivity"
            android:exported="false" />
        <activity
            android:name=".ui.materi.LihatDetailMateriActivity"
            android:exported="false" />
        <activity
            android:name=".ui.materi.TambahMateriActivity"
            android:exported="false" />
        <activity
            android:name=".ui.peserta.TambahPesertaActivity"
            android:exported="false" />
        <activity
            android:name=".ui.peserta.LihatDetailPesertaActivity"
            android:exported="false" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.ProjectIndividu.NoActionBar">
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ProjectIndividu.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>